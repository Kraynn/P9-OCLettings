{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "django",
      "image": "kraynn/ocl:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "environment": [
        {
          "name": "DJANGO_SETTINGS_MODULE",
          "value": "oc_lettings_site.settings"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "static",
          "containerPath": "/app/static"
        }
      ]
    }
  ],
  "volumes": [
    {
      "name": "static",
      "host": {
        "sourcePath": "./static"
      }
    }
  ]
}
